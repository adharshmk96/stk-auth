{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "serve in port 8080",
        "type": "go",
        "request": "launch",
        "mode": "auto",
        "program": "${workspaceFolder}/main.go",
        "env": {
          "SESSION_COOKIE_NAME": "stk_session_id",
          "JWT_SESSION_COOKIE_NAME": "stk_session_token",
          "JWT_EDCA_PRIVATE_KEY_PATH": ".keys/private_key.pem",
          "JWT_EDCA_PUBLIC_KEY_PATH": ".keys/public_key.pem",
        },
        "args": ["serve", "-p", "8080"]
      }
    ]
  }
  